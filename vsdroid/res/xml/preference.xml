<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
  xmlns:android="http://schemas.android.com/apk/res/android">
	<Preference android:title="Status" android:key="key_status" android:selectable="false" android:summary="@string/pref_summery_status"></Preference>
	<CheckBoxPreference android:key="key_reopen_log" android:title="@string/pref_reopen_log"></CheckBoxPreference><CheckBoxPreference android:key="key_caribration" android:title="@string/pref_caribration"></CheckBoxPreference><ListPreference
		android:key="key_vsd_mode" android:defaultValue="0" android:dialogTitle="@string/pref_vsd_mode" android:title="@string/pref_vsd_mode" android:entryValues="@array/list_vsd_mode_values" android:entries="@array/list_vsd_mode"/>
	
	<ListPreference android:key="key_sectors" android:title="@string/pref_sectors" android:dialogTitle="@string/pref_sectors" android:entries="@array/list_sectors" android:entryValues="@array/list_sectors"></ListPreference><EditTextPreference android:defaultValue="1.0" android:title="@string/pref_gymkhana_start" android:dialogTitle="@string/pref_gymkhana_start" android:key="key_gymkha_start"></EditTextPreference>
	<EditTextPreference android:key="key_ip_addr" android:defaultValue="192.168.0.1" android:title="@string/pref_ip_addr" android:dialogTitle="@string/pref_ip_addr"></EditTextPreference>
<CheckBoxPreference android:key="key_eco_mode" android:summary="@string/pref_summery_eco_mode" android:title="@string/pref_eco_mode"></CheckBoxPreference>
<CheckBoxPreference android:key="key_replay_log" android:summary="@string/pref_summery_replay_log" android:title="@string/pref_replay_log"></CheckBoxPreference>

</PreferenceScreen>
